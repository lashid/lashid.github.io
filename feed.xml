---
layout: null
---
<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
  {% assign feed_path = "/feed.xml" %}
  <channel>
    <title> {{ site.title | xml_escape }}  </title>
    <description>{{ site.description | xml_escape }}</description>
    <link>{{ site.url }}{{ site.baseurl }}/</link>
    <atom:link href="{{ feed_path | prepend: site.baseurl | prepend: site.url }}" rel="self" type="application/rss+xml"/>
    <pubDate>{{ site.time | date_to_rfc822 }}</pubDate>
    <lastBuildDate>{{ site.time | date_to_rfc822 }}</lastBuildDate>
    <generator>Jekyll v{{ jekyll.version }}</generator>
    {% for post in site.posts limit:10 %}
      <item>
        <![CDATA[<]]>title<![CDATA[>]]>{{ post.title | xml_escape }}<![CDATA[<]]>/title<![CDATA[>]]>
        <![CDATA[<]]>description<![CDATA[>]]>{{ post.content | xml_escape }}<![CDATA[<]]>/description<![CDATA[>]]>
        <![CDATA[<]]>pubDate<![CDATA[>]]>{{ post.date | date_to_rfc822 }}<![CDATA[<]]>/pubDate<![CDATA[>]]>
        <![CDATA[<]]>link<![CDATA[>]]>{{ post.url | prepend: site.baseurl | prepend: site.url }}<![CDATA[<]]>/link<![CDATA[>]]>
        <![CDATA[<]]>guid isPermaLink="true"<![CDATA[>]]>{{ post.url | prepend: site.baseurl | prepend: site.url }}<![CDATA[<]]>/guid<![CDATA[>]]>
        {% for tag in post.tags %}
        <![CDATA[<]]>category<![CDATA[>]]>{{ tag | xml_escape }}<![CDATA[<]]>/category<![CDATA[>]]>
        {% endfor %}
        {% for cat in post.categories %}
        <![CDATA[<]]>category<![CDATA[>]]>{{ cat | xml_escape }}<![CDATA[<]]>/category<![CDATA[>]]>
        {% endfor %}
        ]]>
      </item>
    {% endfor %}
  </channel>
</rss>
